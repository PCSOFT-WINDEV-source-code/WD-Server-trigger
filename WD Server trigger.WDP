     code : |1-
      //Describe the connection
      gcntHFCS is connection
      
      // Open the window for connecting to the server
      if Open(WIN_CONNECTION) = False then
      	EndProgram()
      end
      
      //External file
      gdsCONTRIBUTOR is data source
      
      //Establish the connection
      HOpenConnection(gcntHFCS)
      
      //Check whether the version of the HF server is version 120055g or later
      if HInfoServer(gcntHFCS,hInfoVI)[[4 to]] < "120055g" then
      	Error(<§@00000000000000000000§>)
      	EndProgram()
      end
      
      //Open the CONTRIBUTOR file
      if HDeclareExternal("./CONTRIBUTOR.FIC",gdsCONTRIBUTOR,gcntHFCS) = False then
      	// Copy the file to the server
      	HCopyFile(gcntHFCS,".\CONTRIBUTOR.FIC","",fExeDir() + "\" + "CONTRIBUTOR.FIC",hCopyIndex)
      	
      	HDeclareExternal("./CONTRIBUTOR.FIC",gdsCONTRIBUTOR,gcntHFCS)
      	
      	// Update the triggers and stored procedures
      	HRefreshSet(gcntHFCS,"psWD_Server_trigger")
      	
      	
      	HCreateServerTrigger(Trigger_Archiving)
      	HCreateServerTrigger(Trigger_ModifDateTime)
      	
      	
      end
