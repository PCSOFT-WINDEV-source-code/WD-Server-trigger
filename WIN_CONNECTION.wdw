     procedures :
      -
        name : ConnectClientServer
        procedure_id : 1219710655544926573
        type_code : 14
        code : |1+
         Procedure ConnectClientServer()
         
         // Parameters of the connection
         gcntHFCS..Provider = hAccessHFClientServer
         gcntHFCS..User = EDT_USER
         gcntHFCS..Password = EDT_PASSWORD
         gcntHFCS..Server = EDT_SERVER + “:” + EDT_PORT_NUMBER
         gcntHFCS..Database = EDT_Database
         gcntHFCS..CryptMethod = hCryptNo
         
         // Establish the connection
         IF HOpenConnection(gcntHFCS) = False THEN
         	Error(“Unable to connect to the server.”,“Check the parameters entered.”)
         	ReturnToCapture()
         END
         
         // No change of connection if the database is empty
         IF NOT EDT_Database ~= “” THEN
         	// Change the connection (for all the files of the analysis of the project)
         	HChangeConnection(“*”,gcntHFCS)
         END
         
